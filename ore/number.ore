Number {
	denominator;
	numerator;
	type;

	new { num;
		./numerator = num
		./denominator = 1
	}

	to_s {; #proxy }
	abs {; #proxy }
	floor {; #proxy }
	ceil {; #proxy }
	round {; #proxy }
	sqrt {; #proxy }
	even? {; #proxy }
	odd? {; #proxy }
	to_i {; #proxy }
	to_f {; #proxy }
	clamp { min, max; #proxy }
}
