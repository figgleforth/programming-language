Number {
	denominator;
	numerator;
	type;

	new { num;
		./numerator = num
		./denominator = 1
	}

	to_s {; #intrinsic }
	abs {; #intrinsic }
	floor {; #intrinsic }
	ceil {; #intrinsic }
	round {; #intrinsic }
	sqrt {; #intrinsic }
	even? {; #intrinsic }
	odd? {; #intrinsic }
	to_i {; #intrinsic }
	to_f {; #intrinsic }
	clamp { min, max; #intrinsic }
}
